#include<bits/stdc++.h>
#define ll long long int
#define pb push_back
using namespace std;

int main(){
    ll t;
    cin>>t;
    while(t--){
        ll n;
        cin>>n;
        ll a[2][n];
        for(ll i=0;i<n;i+=2){
            cout<<2*n-i<<" "<<i+2<<" ";
        }
        cout<<endl;
        for(ll i=0;i<n;i+=2){
            cout<<i+1<<" "<<n+i+1<<" ";
        }
        cout<<endl;
    }
}
