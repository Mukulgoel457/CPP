#include<bits/stdc++.h>
#define ll long long int
#define pb push_back
using namespace std;

int main(){
    ll n,k,c=0,sum=0;
    cin>>n>>k;
    char a[n],b[k];
    for(ll i=0;i<n;i++){
        cin>>a[i];
    }
    for(ll i=0;i<k;i++){
        cin>>b[i];
    }
    for(ll i=0;i<n;i++){
        for(ll j=0;j<k;j++){
            if(a[i]==b[j]){
                c++;
                break;
            }
            else if(j==k-1&&a[i]!=b[j]){
                sum+=(c*(c+1))/2;
                c=0;
            }
        }
    }
    sum+=(c*(c+1))/2;
    cout<<sum<<endl;
}
