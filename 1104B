const ll mod = 1e9 + 7;
ll gcd(ll a, ll b){
  if (b == 0) return a;
  return gcd(b, a%b);
}
bool is_prime[1009];

void sieve_of_eratosthenes(int n)
{
    memset(is_prime, true, sizeof(is_prime));
    is_prime[0] = is_prime[1] = false;
    for (int p = 2; p * p <= n; p++) {
        if (is_prime[p]) {
            for (int i = p * p; i <= n; i += p) {
                is_prime[i] = false;
            }
        }
    }
}


void solve(){
    string a;
    cin>>a;
    ll k=0;
    while(a.length()>0){
            ll z=k;
    for(ll i=0;i<a.length()-1;i++){
        if(a[i]==a[i+1]&&a.length()>0){
            a.erase(i,2);
            k++;
            break;
        }
    }
    if(z==k){break;}
    }
     if(k%2==1){pyes;}
            else{pno;}
}

int main(){
    ll t=1;
    ios::sync_with_stdio(false);
    //cin>>t;
    while(t--){
        solve();
    }
}
