#include<bits/stdc++.h>
#define ll long long int
#define pb push_back
using namespace std;
 
int main(){
    ll t;
    cin>>t;
    while(t--){
            ll n,maxa=0;
    cin>>n;
    ll a[n];
    for(ll i=0;i<n;i++){
        cin>>a[i];
        if(i>0){
            if(a[i]<a[i-1]){
                maxa=max(a[i-1],maxa);
            }
        }
    }
    cout<<maxa<<endl;
    }
}
