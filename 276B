#include <iostream>
#include<algorithm>
using namespace std;

int main() {
string s;
cin>>s;
int n=s.length(),k=1,z=0;
sort(s.begin(),s.end());
for(int i=0;i<n;i++){
    if(s[i]==s[i+1]){k++;}
    else{
            if(k%2!=0){
        z++;
     }k=1;
    }
}
if(z%2==0&&z>0){cout<<"Second";}
else{cout<<"First";}
}
