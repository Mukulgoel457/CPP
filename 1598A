#include<bits/stdc++.h>
#define ll long long int
#define pb push_back
using namespace std;

int main(){
    ll t;
    cin>>t;
    while(t--){
      ll n,k=0;
      cin>>n;
       char a[2][n];
       for(ll i=0;i<2;i++){
         for(ll j=0;j<n;j++){
             cin>>a[i][j];
          }
        }
        for(ll j=0;j<n;j++){
            if(a[0][j]=='0'||a[1][j]=='0'){
                k++;
            }
        }
        if(k==n){cout<<"YES"<<endl;}
        else{cout<<"NO"<<endl;}
    }
}
