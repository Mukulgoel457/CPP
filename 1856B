#include<bits/stdc++.h>
#define ll long long int
#define pb push_back
using namespace std;

int main(){
    ll t;
    cin>>t;
    while(t--){
        ll n,sum=0,k=0;
        cin>>n;
        ll a[n];
        for(ll i=0;i<n;i++){
            cin>>a[i];
            sum+=a[i];
            if(a[i]==1){k++;}
        }
        if(n+k<=sum&&n>1){
            cout<<"YES"<<endl;
        }
        else{cout<<"NO"<<endl;}
    }
}
