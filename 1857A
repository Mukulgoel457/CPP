#include<bits/stdc++.h>
#define ll long long int
using namespace std;
 
int main(){
    ll t;
    cin>>t;
    while(t--){
        ll n,k=0;
        cin>>n;
        ll a[n];
        for(ll i=0;i<n;i++){
            cin>>a[i];
            if(a[i]%2==1){
                k++;
            }
        }
        if(k%2==0){cout<<"YES"<<endl;}
        else{cout<<"NO"<<endl;}
    }
}
