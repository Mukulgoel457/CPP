#include<bits/stdc++.h>
using namespace std;

int main(){
    int t;
    cin>>t;
    while(t--){
        int n,m,k,z=0,x,y;
        cin>>n>>m>>k;
        int a[k][2];
        cin>>x>>y;
        for(int i=0;i<k
        ;i++){
            cin>>a[i][0]>>a[i][1];
        }
        for(int i=0;i<k;i++){
            if((x+y)%2==(a[i][0]+a[i][1])%2){
                cout<<"NO"<<endl;z++;break;
            }
        }
        if(z==0){cout<<"YES"<<endl;}
    }
}
