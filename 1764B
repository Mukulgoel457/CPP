#include<bits/stdc++.h>
#define ll long long int
#define pb push_back
const int n1=1e9+7;
using namespace std;

int main(){
    ll t;
    cin>>t;
    while(t--){
        ll n;
        cin>>n;
        ll a[n];
        for(ll i=0;i<n;i++){
            cin>>a[i];
        }
        ll res=0;
        for(ll i=0;i<n;i++){
                res=__gcd(res,a[i]);
            }
    cout<<(a[n-1]/res)<<endl;
    }
}
